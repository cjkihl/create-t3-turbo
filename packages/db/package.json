{
  "name": "@acme/db",
  "version": "0.1.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "type-check": "tsc --noEmit",
    "pg:push": "pnpm echo \"Push for postgres not yet supported\"",
    "drizzle-kit": "pnpm with-env drizzle-kit",
    "pg:generate": "pnpm with-env drizzle-kit generate:pg",
    "pg:migrate": "pnpm with-env node -r @swc-node/register pg/run-migrations.ts",
    "pg:reset": "pnpm with-env node -r @swc-node/register pg/run-reset.ts",
    "mysql:push": "pnpm with-env drizzle-kit push:mysql",
    "mysql:generate": "pnpm with-env drizzle-kit generate:mysql",
    "mysql:migrate": "pnpm with-env node -r @swc-node/register mysql2/run-migrations.ts",
    "mysql:reset": "pnpm with-env node -r @swc-node/register mysql/run-reset.ts",
    "dev": "pnpm with-env drizzle-kit studio --port 5556",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.4.26",
    "@planetscale/database": "^1.8.0",
    "drizzle-orm": "^0.27.1",
    "mysql2": "^3.5.1",
    "postgres": "^3.3.5"
  },
  "devDependencies": {
    "@swc-node/register": "^1.6.6",
    "@types/pg": "^8.10.2",
    "dotenv-cli": "^7.2.1",
    "drizzle-kit": "^0.19.3",
    "pg": "^8.11.1",
    "type-fest": "^3.13.0",
    "typescript": "^5.1.6"
  }
}
